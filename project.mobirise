              },
              "&:hover .table-heading": {
                "background-color": "@tHeadingHover"
              }
            },
            ".mbr-iconfont": {
              "margin": "0 0.5em"
            },
            ".table-heading": {
              "transition": "0.3s ease-out all",
              "display": "block",
              "width": "100%",
              "padding": "0.6em 4rem",
              "background-color": "@headingBg",
              "color": "#ffffff",
              "text-align": "center"
            },
            ".table-purchase": {
              ".pricing-value": {
                "vertical-align": "top",
                "display": "inline-block"
              },
              ".table-pricing": {
                "display": "inline-block"
              },
              "padding": "0 4rem",
              "margin": "2.5em 0 0 0"
            },
            ".table-pricing,.pricing-value h1": {
              "text-align": "left"
            },
            ".table-pricing": {
              "text-align": "center"
            },
