            },
            ".container-fluid": {
              "padding": "0 3rem"
            },
            ".table-wrap": {
              "overflow": "hidden",
              "border-radius": "3%",
              "background-color": "@cardColor",
              "transition": "0.3s ease-out all",
              "padding": "0",
              "margin-bottom": "1em",
              ".table-pricing": {
                "margin": "0 0 0.4em 0"
              },
              "ul": {
                "word-wrap": "break-word",
                "padding": "0 4rem",
                "margin": "0 0 2rem 0",
                "list-style": "none",
                "& when (@showButtons)": {
                  "margin": "0"
                },
                "li": {
                  "display": "flex",
                  "flex-direction": "row",
                  "padding": "0.7em 0",
                  "align-items": "flex-end",
                  "span": {
                    "font-size": "32px",
                    "color": "rgb(0, 0, 0)"
                  }
                }
              },
              "ul:last-child": {
                "padding-bottom": "2rem"
              },
              "&:hover": {
